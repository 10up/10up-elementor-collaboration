{
	"name": "elementor",
	"version": "0.0.3",
	"author": "Pojo Team",
	"homepage": "http://pojo.me/",
	"devDependencies": {
		"browserify": "^13.0.0",
		"browserify-shim": "^3.8.12",
		"grunt": "^0.4.5",
		"grunt-browserify": "^5.0.0",
		"grunt-exorcise": "~2.1.1",
		"grunt-contrib-jshint": "^1.0.0",
		"grunt-contrib-watch": "^1.0.0",
		"grunt-postcss": "~0.8.0",
		"grunt-contrib-uglify": "~1.0.1",
		"grunt-contrib-sass": "~1.0.0",
		"autoprefixer": "~6.3.5",
		"cssnano": "~3.5.2",
		"grunt-checktextdomain": "~1.0.0",
		"grunt-pot": "~0.2.1",
		"remapify": "2.1.0",
		"matchdep": "~1.0.1"
	},
	"browserify": {
		"transform": [
			"browserify-shim"
		]
	},
	"browserify-shim": {
		"jquery": "global:jQuery",
		"underscore": "global:_",
		"backbone": {
			"exports": "global:Backbone",
			"depends": [
				"jquery",
				"underscore"
			]
		},
		"marionette": {
			"exports": "global:Marionette",
			"depends": [
				"jquery",
				"underscore",
				"backbone"
			]
		}
	}
}
